{
  "name": "@alchemist/application",
  "description": "Alchemist, the node based code generation tool",
  "version": "0.1.0",
  "main": "app.js",
  "productName": "Alchemist",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build-reasons": "webpack --display-reasons",
    "electron": "electron app.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@treacherous/core": "^0.17.7",
    "@treacherous/decorators": "^0.2.4",
    "@treacherous/view": "^0.4.4",
    "@treacherous/vue": "^0.4.2",
    "@alchemist/core": "^0.5.1",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "bulma": "^0.7.5",
    "bulmaswatch": "^0.7.5",
    "bulma-extensions": "^6.2.7",
    "electron-edge-js": "^10.11.0",
    "highlight.js": "^9.15.10",
    "lodash": "^4.17.15",
    "uuid": "^3.3.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3",
    "vue2-ace-editor": "^0.0.14",
    "vue-toasted": "^1.1.27",
    "liquor-tree": "^0.2.65",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "interactjs": "^1.6.2",
    "vue-context": "^4.0.3",
    "fs-extra": "^8.1.0",
    "core-js": "^3.2.1"
  },
  "devDependencies": {
    "@types/uuid": "^3.4.5",
    "@types/ace": "^0.0.42",
    "@types/pubsub-js": "^1.5.18",
    "@types/fs-extra": "^8.0.0",
    "@types/vue-router": "^2.0.0",
    "@types/lodash-es": "^4.17.3",
    "cross-env": "^6.0.2",
    "node-sass": "^4.12.0",
    "css-loader": "^3.2.0",
    "sass-loader": "^8.0.0",
    "file-loader": "^4.2.0",
    "url-loader": "^2.1.0",
    "ts-loader": "^6.2.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-devtools": "^5.1.3",
    "vue-template-compiler": "^2.6.10",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "electron-builder": "^20.38.5",
    "electron": "^3.1.2"
  },
  "build": {
    "appId": "com.alchemist",
    "productName": "Alchemist",
    "compression": "store",
    "files": [
      "!node_modules/**/*",
      "public/**/*",
      "app.js",
      "package.json",
      {
        "from": "node_modules",
        "to": "node_modules",
        "filter": [
          "vue-devtools/**/*",
          "fs-extra/**/*",
          "universalify/**/*",
          "graceful-fs/**/*",
          "jsonfile/**/*",
          "electron-edge-js/**/*"
        ]
      }
    ],
    "win": {
      "icon": "public/favicon.png"
    }
  }
}
